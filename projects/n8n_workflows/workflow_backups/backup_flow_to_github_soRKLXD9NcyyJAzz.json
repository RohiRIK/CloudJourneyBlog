      "parameters": {
        "jsCode": "/**\n * Deeply compares two objects to find differences, skipping specified keys.\n * @param {object} previousState The original object state.\n * @param {object} currentState The new object state to compare against.\n * @param {string[]} keysToIgnore An array of keys to ignore during comparison.\n * @returns {object} An object describing the differences between the states.\n */\nfunction compareObjects(previousState, currentState, keysToIgnore = []) {\n    const diff = {};\n\n    const previousObj = (previousState && typeof previousState === 'object' && !Array.isArray(previousState)) ? previousState : {};\n    const currentObj = (currentState && typeof currentState === 'object' && !Array.isArray(currentState)) ? currentState : {};\n\n    const previousKeys = Object.keys(previousObj);\n    const currentKeys = Object.keys(currentObj);\n    const allKeys = new Set([...previousKeys, ...currentKeys]);\n\n    for (const key of allKeys) {\n        // --- NEW: IGNORE LOGIC ---\n        // If the current key is in our ignore list, skip to the next key.\n        if (keysToIgnore.includes(key)) {\n            continue;\n        }\n\n        const previousValue = previousObj[key];\n        const currentValue = currentObj[key];\n        const inPrevious = previousObj.hasOwnProperty(key);\n        const inCurrent = currentObj.hasOwnProperty(key);\n\n        if (inPrevious && !inCurrent) {\n            diff[key] = { status: 'REMOVED', oldValue: previousValue };\n        } else if (!inPrevious && inCurrent) {\n            diff[key] = { status: 'ADDED', newValue: currentValue };\n        } else if (previousValue !== currentValue) {\n            const isPreviousValueObject = typeof previousValue === 'object' && previousValue !== null && !Array.isArray(previousValue);\n            const isCurrentValueObject = typeof currentValue === 'object' && currentValue !== null && !Array.isArray(currentValue);\n\n            if (isPreviousValueObject && isCurrentValueObject) {\n                // Pass the ignore list down to the recursive call\n                const nestedDiff = compareObjects(previousValue, currentValue, keysToIgnore);\n                if (Object.keys(nestedDiff).length > 0) {\n                    diff[key] = { status: 'MODIFIED', changes: nestedDiff };\n                }\n            } else {\n                diff[key] = { status: 'MODIFIED', oldValue: previousValue, newValue: currentValue };\n            }\n        }\n    }\n    return diff;\n}\n\n\n// --- Main Execution ---\n\n// Define the keys you want to ignore. These are common ones for n8n workflows.\n// 'position' - visual layout on the canvas\n// 'updatedAt', 'createdAt' - timestamps that always change\nconst KEYS_TO_IGNORE = ['updatedAt', 'createdAt', 'position'];\n\nconst oldJson = $input.item.json.backuped_json;\nconst newJson = $input.item.json.updated_json;\n\n// Find the differences, passing the list of keys to ignore.\nconst differences = compareObjects(oldJson, newJson, KEYS_TO_IGNORE);\n\nif (Object.keys(differences).length > 0) {\n    return {\n        update_needed: true,\n        reason: \"Comparison found functional changes. The backup needs to be updated.\",\n        changes: differences\n    };\n} else {\n    return {\n        update_needed: false,\n        reason: \"No functional changes found. The backup is current.\"\n    };\n}"
      },
