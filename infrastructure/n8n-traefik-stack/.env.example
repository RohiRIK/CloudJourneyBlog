# Domain Configuration
TRAEFIK_DOMAIN=example.com
N8N_SUBDOMAIN=n8n

# Traefik ACME (Let's Encrypt) Configuration
TRAEFIK_ACME_EMAIL=your-email@example.com

# Cloudflare DNS API Configuration (for DNS challenge)
CLOUDFLARE_DNS_API_TOKEN=your-cloudflare-token

# PostgreSQL Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=change-this-password
POSTGRES_DB=n8n

# n8n Configuration
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-this-password
N8N_ENCRYPTION_KEY=change-this-to-a-random-string
